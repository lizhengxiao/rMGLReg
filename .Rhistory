u1 <- U[1]
u2 <- U[2]
empc <- C.n(u = cbind(u1, u2), X = XY)
fitc <- do.call(pGM180, args = list(U = cbind(u1, u2),
param = param))
out <- (fitc - empc)^2
out
}
